			response.put('status','error');
			response.put('message','Departure date must be ahead of Arrival date');
		}
		else if(arrivalDate.getDay() == depDate.getDay())
		{
			if(depDate1.replaceAll("T"," ").toString("MM-dd-yyyy HH:mm:ss","America/Fort_Wayne") == arrivalDate1.replaceAll("T"," ").toString("MM-dd-yyyy HH:mm:ss","America/Fort_Wayne"))
			{
				response.put('status','error');
				response.put('message','Departure date must be ahead of Arrival date');
			}
			else if(depDate1.replaceAll("T"," ").toString("MM-dd-yyyy HH:mm:ss","America/Fort_Wayne").timeBetween(arrivalDate1.replaceAll("T"," ").toString("MM-dd-yyyy HH:mm:ss","America/Fort_Wayne")) == "")
			{
				response.put('status','success');
			}
			else
			{
				response.put('status','error');
				response.put('message','Departure date must be ahead of Arrival date');
			}
		}
		else
